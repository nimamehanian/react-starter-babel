{
  "name": "start",
  "version": "0.0.1",
  "description": "start",
  "main": "./src/index.js",
  "author": "Nima Mehanian",
  "license": "MIT",
  "private": true,
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      "react-hot-loader/babel"
    ]
  },
  "scripts": {
    "cp-index": "cp ./src/index.html ./dist",
    "cp-favicon": "cp ./src/images/favicon.png ./dist",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "yarn run cp-index && yarn run cp-favicon && webpack --progress",
    "build": "yarn run clean && yarn run copy",
    "start": "yarn run build && webpack-dev-server",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "deploy": "NODE_ENV=production yarn build && firebase deploy"
  },
  "dependencies": {
    "connected-react-router": "6.5.2",
    "history": "4.10.1",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-hot-loader": "4.12.14",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-spring": "8.0.27",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-observable": "1.2.0",
    "rxjs": "6.5.3",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.6.2",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.15.1",
    "file-loader": "4.2.0",
    "jest": "24.9.0",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  }
}
